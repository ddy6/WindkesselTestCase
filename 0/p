    brachiocephalic
    {
    type        windkesselPressure;
    Rp          4.749e05; //increase to decrease mean flow to branch
    C           7.370e-07; //increase to increase buffer cap, smoother flow and reduced pulse pressure
    Rc          4.5460e+03; //should be similar for outlets of similar size; smaller branch means higher impedance, but doesn't make much difference at this level
    P_downstream    0.4;      //keep constant; venous pressure
    initialPC   10.0;     //keep constant; step 0 pressure, set to 80mmHg    
    value       uniform     10; //step 0 value, matches initialPC
    }

